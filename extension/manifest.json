{
  "name": "Send to Trello",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Sends a quick note to Trello.",
  "icons": { "128": "paperplane_128.png" },
  "permissions": ["http://*.google.com/", "https://*.google.com/"],
  
  "browser_action": {
    "default_title": "",
    "default_icon": "paperplane.png",
    "default_popup": "popup.html"
  }
}